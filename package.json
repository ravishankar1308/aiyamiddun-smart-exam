{
  "name": "node-express-api-ts",
  "description": "Simple api sample in Node",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'npm run build && npm run start'",
    "dev:frontend": "npm run dev --prefix frontend",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "firebase": "^12.6.0",
    "lucide-react": "^0.555.0",
    "mysql2": "^3.15.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5"
  }
}
